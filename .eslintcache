ia-describedby=\"transition-modal-description\"\n        className={classes.modal}\n        onClose={handleClose}\n        closeAfterTransition\n        BackdropComponent={Backdrop}\n        BackdropProps={{\n          timeout: 500,\n        }}\n        open={modal}\n      >\n        <Fade in={modal}>\n\n          <form onSubmit={event => {\n            event.preventDefault();\n            handleLogout();\n          }}\n            className='register-container'\n            style={{\n              flexDirection: 'column',\n              justifyContent: 'space-between',\n              alignItems: 'center'\n            }}\n          >\n            <div style={{\n              fontFamily:'Poppins',\n              marginBottom: 12\n            }}>\n              Confirm Logout?\n            </div>\n            <div style={{\n              display: 'flex',\n              flexDirection: 'row',\n              justifyContent: 'space-around',\n              width: \"100%\"\n            }}>\n              <Button onClick={() => setModal(false)}\n                variant='contained' color='primary'\n                className='user-input-btn'\n                color='default'\n                style={{\n                  color: 'black',\n                  fontWeight: 'bold'\n                }}\n              >Cancel</Button>\n              <Button\n                variant='contained' color='primary'\n                type='submit'\n                className='user-input-btn'\n                style={{\n                  color: 'white',\n                  fontWeight: 'bold'\n                }}\n              >OK</Button>\n\n            </div>\n\n          </form>\n        </Fade>\n      </Modal>\n    </>\n  );\n};\n\nexport default ConfirmModal;\n\n/* <div className='login__container'>\n  <form onSubmit={event => event.preventDefault()}>\n    <label>I Am Login Form</label>\n    <input name='email' type='email' value={login.email} onChange={event => handleChange('email', event.target.